package backEnd;
import java.sql.Connection;
import java.sql.ResultSet;
import java.sql.SQLException;
import java.sql.Statement;

import frontEnd.LoginPage;

public class RetrievedData {
	Connection con = Model.connect();
	Statement stmtT;
	Statement stmt1; 
	ResultSet rsT; 
	ResultSet rsF; 
	String userTitle, userFirstName, userLastN, userEmail, userPassword; 
	public RetrievedData() throws SQLException {
		stmtT=con.createStatement();
		rsT =stmtT.executeQuery("select personal_title from registrationAtm");
		rsF = stmt1.executeQuery("select personal_firstName from registrationAtm"); 
		while (rsT.next()&& rsF.next()) {
			userTitle= rsT.getString(1); 
			userFirstName = rsF.getString(2);
			
			System.out.println("The users "+ userTitle+ userFirstName);
			
		}
		rsT.close();
		stmtT.close();
		rsF.close();
		stmt1.close();
		
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		new LoginPage();

	}

}
