package backEnd;
import java.sql.Connection;
import java.sql.ResultSet;
import java.sql.SQLException;
import java.sql.Statement;

import frontEnd.LoginPage;

public class RetrievedData {
	Connection con = Model.connect();
	Statement stmt; 
	ResultSet rs; 
	String userFirstName, userEmail; 
	public RetrievedData() throws SQLException {
		stmt=con.createStatement();
		rs =stmt.executeQuery("select personal_firstName personal_email from registrationAtm");
		while (rs.next()) {
			userFirstName = rs.getNString(1);
			userEmail = rs.getString(2);
			System.out.println("The users "+ userFirstName);
			
		}
		rs.close();
		stmt.close();
		
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		new LoginPage();

	}

}
